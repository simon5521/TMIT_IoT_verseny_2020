package UC
import "interfaces"
statechart UCStatechart [
	port HWFault : requires UCFault
	port Fault : provides UCFault
] {
	transition from On to Off when HWFault.shutdown
	transition from Entry0 to On
	region UC {
		initial Entry0
		state Off {
			entry / raise Fault.shutdown;
		}
		state On
	}
}